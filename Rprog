makeCacheVector <- function(x = numeric()) {
  m <- NULL
  set <- function(y) {
    x <<- y
    m <<- NULL
  }
  get <- function() x
  setmean <- function(mean_value) m <<- mean_value
  getmean <- function() m
  list(set = set, get = get, setmean = setmean, getmean = getmean)
}

cacheMean <- function(x, ...) {
  m <- x$getmean()
  if (!is.null(m)) {
    message("getting cached mean")
    return(m)
  }
  data <- x$get()
  m <- mean(data, ...)
  x$setmean(m)
  m
}
